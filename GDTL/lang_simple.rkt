(define-extended-language LContext
  L
  (redexContext RC ::= 
    hole
    (TermApp RC et)
    (TermApp ev RC)
    (TermPi x RC eT)
    (TermEp ep RC)
    (TermSucc RC)
    (TermNatElim i RC eT et_Z x_1 x_2 et_S)
    (TermNatElim i ev RC et_Z x_1 x_2 et_S)
    (TermNatElim i ev eV RC x_1 x_2 et_S)

